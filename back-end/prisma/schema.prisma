generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Report {
  id  String @id @default(cuid())
  title String
  description String 
  images String[] @default([])
  lat  Float 
  long Float
  createdAt DateTime @default(now())
}

